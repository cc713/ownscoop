{
    "homepage": "https://anytxt.net/",
    "description": "A local document search application",
    "version": "1.2.540",
    "license": "GPL-3.0-or-later",
    "url": "https://sourceforge.net/projects/anytxt/files/AnyTXT.Searcher.1.2.540.exe",
    "hash": "sha1:f095be4f5b1deef43a53e0a2bdf0218904dff4be",
    "installer": {
        "args": [
            "/verysilent",
            "/dir=\"$dir\"",
            "/tasks=\"fileassoc\""
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/VERYSILENT",
            "/CLOSEAPPLICATIONS",
            "/FORCECLOSEAPPLICATIONS"
        ]
    },
    "bin": "ATGUI.exe",
    "checkver": {
        "url": "https://anytxt.net/download/",
        "regex": "AnyTXT.Searcher.([\\d.]+).exe"
    },
    "autoupdate": {
        "url": "https://sourceforge.net/projects/anytxt/files/AnyTXT.Searcher.$version.exe"
    }
}
